p#notice = notice
h1 Questions
- @questions.each do |question|
  .question
    .box
      .phone_number= question.phone_number
      .message
        p= question.message
        .answers-number= link_to question.answers.count, question_answers_path(question)
    .options
      .btn-answer= link_to 'Answer', new_question_answer_path(question)
